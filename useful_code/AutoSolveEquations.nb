fsolve[ff_]:=Solve[ff==0,getc[ff]][[1]]
getc[ff_]:=Module[{lis,rst},
                  lis=Level[{ff},-1]//Union;
                  Do[If[MemberQ[{cc},Head[lis[[ii]]]],rst=lis[[ii]];Break[]],{ii,lis//Length}];
                  rst]
rep={};
While[Length[ceqn]>1,
      rep1=fsolve[ceqn[[2]]];
      rep=rep/.rep1//Expand;
      rep=Join[rep,rep1];
      ceqn=ceqn//.rep//Expand//Union;
      Print[{rep,ceqn//Length,rep//Length}]]
rep
